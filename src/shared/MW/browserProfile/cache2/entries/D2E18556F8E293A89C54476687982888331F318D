/* #############################   GENERALS   ################################### */
/* ------------------------------------------------------------------------------ */

.sqwForm{ margin: 0; padding: 0; position: relative; } /* reset stuff */

  /* Some generals and more resets */
  .sqwForm fieldset,
  .sqwForm .formBlock{ border: none; margin: 10px 0 10px 0; padding: 10px; width:auto;}
    .sqwForm fieldset legend{ margin: 0; padding: 0; }

  /* This are the main units that contain form elements */
  .sqwForm .ctrlHolder,
  .sqwForm .buttonHolder{ margin: 0; padding: 0; clear: both; }

  /* Clear all floats */
  .sqwForm:after,
    .sqwForm .buttonHolder:after,
    .sqwForm .ctrlHolder:after,
      .sqwForm .ctrlHolder .multiField:after,
        .sqwForm .inlineLabel:after{ content: "."; display: block; height: 0; line-height: 0; font-size: 0; clear: both; min-height: 0; visibility: hidden; }

  .sqwForm label,
  .sqwForm button{ cursor: pointer; }

/* ------------------------------------------------------------------------------ */
/* ##########################   DEFAULT LAYOUT   ################################ */
/* ------------------------------------------------------------------------------ */
/*       Styles for form controls where labels are above the input elements       */
/* ------------------------------------------------------------------------------ */

  .sqwForm label,
  .sqwForm .label{ display: block; float: none; margin: 0 0 .5em 0; padding: 0; line-height: 100%; width: auto; }

  .sqwForm .labelRadio { margin-top:2px; float:left; margin-right:4px; }

  /* Float the input elements */
  .sqwForm .textInput,
  .sqwForm .fileUpload,
  .sqwForm .selectInput,
  .sqwForm select,
  .sqwForm textarea{
    float: left; 
    width: 53%; 
    margin: 0; 

    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
  }
      
  .sqwForm .textInput.widthShort,
  .sqwForm .fileUpload.widthShort,
  .sqwForm .selectInput.widthShort,
  .sqwForm select.widthShort,
  .sqwForm textarea.widthShort {
    width:19%;
  }
      
  .sqwForm .textInput.widthLong,
  .sqwForm .fileUpload.widthLong,
  .sqwForm .selectInput.widthLong,
  .sqwForm select.widthLong,
  .sqwForm textarea.widthLong {
    width:30%;
  }

    /* Read-Only output */
    .sqwForm .readOnlyLabel{ margin: 0; font-size: 1em; font-weight: bold; }
    .sqwForm .readOnly{ font-size: .85em; }
      .sqwForm .readOnly .choiceLabel{ color: #777; text-transform: uppercase; font-size: .75em; letter-spacing: .15em; }

      /* Postition the hints */
      .sqwForm .formHint{ float: right; width: 43%; margin: 0; clear: none; }

      /* Position the elements inside combo boxes (multiple inputs/selects/checkboxes/radio buttons per unit) */
      .sqwForm .ctrlHolder ul{ float: left; width: 53%; margin: 0; padding: 0; }
        .sqwForm .ctrlHolder ul li{ margin: 0 0 .5em 0; list-style: none;}
        .sqwForm .ctrlHolder ul li:hover{ background-color: transparent;} /* fix for crush ".ui-corner-all li:hover" in jquery-ui */
          .sqwForm .ctrlHolder ul li label{ margin: 0; float: none; display: block; overflow: visible; }
        /* Alternate layout */
        .sqwForm .ctrlHolder ul.alternate li{ float: left; width: 30%; margin-right: 3%; }
          .sqwForm .ctrlHolder ul.alternate li label{ float: none; display: block; width: 98%; }
            .sqwForm .ctrlHolder ul .textInput,
            .sqwForm .ctrlHolder ul .selectInput,
            .sqwForm .ctrlHolder ul select,
            .sqwForm .ctrlHolder ul.alternate .textInput,
            .sqwForm .ctrlHolder ul.alternate .selectInput,
            .sqwForm .ctrlHolder ul.alternate select{ width: 98%; margin-top: .5em; display: block; float: none; }

        /* Required fields asterisk styling */
        .sqwForm label em,
        .sqwForm .label em{ float: left; width: 1em; margin: 0 0 0 -1em; }

/* ------------------------------------------------------------------------------ */
/* #########################   ALTERNATE LAYOUT   ############################### */
/* ------------------------------------------------------------------------------ */
/*    Styles for form controls where labels are in line with the input elements   */
/*    Set the class of the parent (preferably to a fieldset) to .inlineLabels     */
/* ------------------------------------------------------------------------------ */

      .sqwForm .inlineLabels label,
      .sqwForm .inlineLabels .label,
      .sqwForm .inlineLabels .readOnlyLabel{ float: left; margin:4px 0 0 0; padding: 0; line-height: 18px; height:18px; position: relative; width: 34%; *width:33%; }

      .sqwForm .inlineLabels .readOnlyLabel{ margin: 0; }

      /* Float the input elements */
      .sqwForm .inlineLabels .textInput,
      .sqwForm .inlineLabels .fileUpload,
      .sqwForm .inlineLabels .selectInput,
      .sqwForm .inlineLabels select,
      .sqwForm .inlineLabels textarea{ float: left; width: 64%; }
      
      .sqwForm .inlineLabels .textInput.widthShort,
      .sqwForm .inlineLabels .fileUpload.widthShort,
      .sqwForm .inlineLabels .selectInput.widthShort,
      .sqwForm .inlineLabels select.widthShort,
      .sqwForm .inlineLabels textarea.widthShort {
		width:20%;
      }
      
      .sqwForm .inlineLabels .textInput.widthLong,
      .sqwForm .inlineLabels .fileUpload.widthLong,
      .sqwForm .inlineLabels .selectInput.widthLong,
      .sqwForm .inlineLabels select.widthLong,
      .sqwForm .inlineLabels textarea.widthLong {
      	width:42%;
      }
      
      .sqwForm .inlineLabels .textInput.widthEqual,
      .sqwForm .inlineLabels .fileUpload.widthEqual,
      .sqwForm .inlineLabels .selectInput.widthEqual,
      .sqwForm .inlineLabels select.widthEqual,
      .sqwForm .inlineLabels textarea.widthEqual {
      	width:31%;
      }

    /* Postition the hints */
    .sqwForm .inlineLabels .formHint{ clear: both; float: none; width: auto; margin-left: 34%; position: static; }

    /* Position the elements inside combo boxes (multiple inputs/selects/checkboxes/radio buttons per unit) */
    .sqwForm .inlineLabels .ctrlHolder ul{ float: left; width: 66%; margin:0 !important; padding:0 !important; }
    .sqwForm .inlineLabels.bottom .ctrlHolder ul { width: 67%; }
      .sqwForm .inlineLabels .ctrlHolder ul li{ margin: .5em 0; }
        .sqwForm .inlineLabels .ctrlHolder ul li label{ float: none; display: block; width: 100%; }
      /* Alternate layout */
      /*.sqwForm .inlineLabels ul.alternate{ width:100% }*/
      .sqwForm .inlineLabels .ctrlHolder ul.alternate li{ margin-left: 3%; margin-top: .25em; }
          .sqwForm .inlineLabels .ctrlHolder ul li label .textInput,
          .sqwForm .inlineLabels .ctrlHolder ul li label textarea,
          .sqwForm .inlineLabels .ctrlHolder ul li label select{ float: none; display: block; width: 98%;  }

    .sqwForm .inlineLabels .readOnly{ float: right; width: 66%; }

    /* Required fields asterisk styling */
    .sqwForm .inlineLabels label em,
    .sqwForm .inlineLabels .label em{ display: block; float: none; margin: 0; position: absolute; right: 0; }

