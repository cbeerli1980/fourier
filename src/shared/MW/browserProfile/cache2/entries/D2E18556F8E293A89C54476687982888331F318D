/* ########################### Additional Stuff ################################ */
/* ----------------------------------------------------------------------------- */

  /* Generals */

      .sqwForm .secondaryAction{ float: left; }

      /* .inlineLabel is used for inputs within labels - checkboxes and radio buttons */
      .sqwForm .inlineLabel input,
      .sqwForm .inlineLabels .inlineLabel input,
      .sqwForm .blockLabels .inlineLabel input,
      /* class .inlineLabel is depreciated */
      .sqwForm label input{ float: none; display: inline; margin: 0; padding: 0; border: none; }

      .sqwForm .buttonHolder .inlineLabel,
      .sqwForm .buttonHolder label{ float: left; margin: .5em 0 0 0;padding:0; width: auto; max-width: 60%; text-align: left; }

      /* When you don't want to use a label */
      .sqwForm .inlineLabels .noLabel ul{ margin-left: 34%; /* Match to width of label + gap to field */ }

      /* Classes for control of the widths of the fields */
      .sqwForm .small { width: 30% !important; }
      .sqwForm .medium{ width: 45% !important; }
      .sqwForm .large {  } /* Large is default and should match the value you set for .textInput, textarea or select */
      .sqwForm .auto  { width: auto !important; height: auto !important; }
      .sqwForm .small,
      .sqwForm .medium,
      .sqwForm .auto{ margin-right: 4px; }



