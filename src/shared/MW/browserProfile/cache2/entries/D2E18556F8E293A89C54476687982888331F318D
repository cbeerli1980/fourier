    .sqwForm .ctrlHolder.focused{ background: #96B343; }

    .sqwForm .buttonHolder{ background: #f3f3f3; color:#fff; text-align: right; margin: 1.5em 0 0 0; padding: 1.5em;}

      .sqwForm .buttonHolder .primaryAction:active{ position: relative; top: 1px; }
      .sqwForm .secondaryAction            { text-align: left; }
      .sqwForm button.secondaryAction      { background: transparent; border: none; color: #FFF; margin: 1.25em 0 0 0; padding: 0; }

        .sqwForm .inlineLabels label em,
        .sqwForm .inlineLabels .label em{ font-style: normal; font-weight: bold; }
        .sqwForm label small{ font-size: .75em; color: #777; }

      .sqwForm .textInput,
      .sqwForm .selectInput,
      .sqwForm textarea    { padding: 4px 2px; height:26px; border: 1px solid #b5b5b5; background: #FFF; color: #000; margin-left:2%;}
      .sqwForm textarea    { height: 12em;}
      
      .sqwForm .textInput:focus,
      .sqwForm textarea:focus{ border: 1px solid #187193; background-color: #FFF; color: #000;}
	  
	  .sqwForm .disabled, .sqwForm .disabled:focus {
		  color: #999;
		  background-color: #eee;
		  border-color: #ccc;
		  -webkit-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.025);
		  -moz-box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.025);
		  box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.025);
	  }

	  .sqwForm .noField { padding: 4px 2px; min-height:18px; color: #000; margin-left:2%; font-weight:normal;}
      

      .sqwForm .ctrlHolder.error textarea,
      .sqwForm .ctrlHolder.error .selectInput,
      .sqwForm .ctrlHolder.error .textInput{border: 1px solid #c20f2e;}
      .sqwForm .ctrlHolder.focused.error textarea,
      .sqwForm .ctrlHolder.focused.error .textInput{border: 3px solid #c20f2e;}

      .sqwForm .ctrlHolder.focused textarea,
      .sqwForm .ctrlHolder.focused .textInput{border: 3px solid #187193;}

      /* Get rid of the 'glow' effect in WebKit, optional */
      .sqwForm .ctrlHolder .textInput:focus,
      .sqwForm .ctrlHolder textarea:focus{ outline: none; }

      .sqwForm .formHint                    { font-size: .85em; color: #777; }
      .sqwForm .inlineLabels .formHint      { padding-top: .5em; }
